[[model]]
name: model-7
author: Sanmitra Ghosh
desc: Check associated model defintion document
# Initial values
ikr.C2 = 1
ikr.C1 = 0
ikr.O = 0

#
# Simulation engine variables
#
[engine]
time = 0 bind time
pace = 0 bind pace

#
# Membrane potential
#
[membrane]
V = engine.pace
    desc: membrane potential
    in [mV]

[nernst]
EK = -88 [mV]

#
# Model from Kylie's mex file (HH)
#
[ikr]
use membrane.V
IKr = p9 * O * (V - nernst.EK)

I = 1 - (C1 + C2 + O)

am = p1 * exp( p2 * V)
bm = p3 * exp(-p4 * V)
a1 = p5 * exp( p6 * V)
b1 = p7 * exp(-p8 * V)


dot(C2) = bm*C1 - 2*am*C2
dot(C1) = 2*am*C2 + 2*bm*O - C1*(bm+am)
dot(O) = am*C1 + b1*I -O*(a1 + 2*bm)

p1 = 2.26e-4 [1/ms]
p2 = 0.06990 [1/mV]
p3 = 3.45e-5 [1/ms]
p4 = 0.05462 [1/mV]
p5 = 0.08730 [1/ms]
p6 = 8.91e-3 [1/mV]
p7 = 5.15e-3 [1/ms]
p8 = 0.03158 [1/mV]
p9 = 0.15240 [mS/uF]

n_params =9
