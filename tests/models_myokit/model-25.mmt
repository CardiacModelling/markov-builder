p15 = 5.15e-3 [1/ms]
p16 = 0.03158 [1/mV]
p17 = 5.15e-3 [1/ms]
p18 = 0.03158 [1/mV]
p19 = 5.15e-3 [1/ms]
p20 = 0.03158 [1/mV]
p21 = 0.15240 [mS/uF]

n_params = 21
